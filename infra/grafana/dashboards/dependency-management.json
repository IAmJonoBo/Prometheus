{
  "title": "Prometheus Dependency Management",
  "uid": "prom-deps-001",
  "slug": "dependency-management",
  "tags": [
    "prometheus-os",
    "observability",
    "dependencies",
    "auto-sync"
  ],
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Auto-Sync Success Rate",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(dependency_auto_sync_success_total[24h]) / rate(dependency_auto_sync_runs_total[24h])",
          "legendFormat": "value",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "last"
          ],
          "fields": ""
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "title": "Guard Violations",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(dependency_guard_violations_total)",
          "legendFormat": "value",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "last"
          ],
          "fields": ""
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "title": "Rollback Events",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(dependency_auto_sync_rollback_total[24h]))",
          "legendFormat": "value",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "last"
          ],
          "fields": ""
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "table",
      "title": "Auto-Sync Timing",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(dependency_auto_sync_duration_seconds_bucket[5m]))",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "Update Trends",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "dependency_updates_available by (package)",
          "format": "table",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Preflight Failures",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 17
      },
      "targets": [
        {
          "expr": "sum(dependency_preflight_failures_total)",
          "legendFormat": "value",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "last"
          ],
          "fields": ""
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "title": "Mirror Health",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 17
      },
      "targets": [
        {
          "expr": "dependency_mirror_signature_valid_ratio",
          "legendFormat": "value",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "last"
          ],
          "fields": ""
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "title": "Model Registry Checks",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 17
      },
      "targets": [
        {
          "expr": "sum(model_registry_signature_checks_total)",
          "legendFormat": "value",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "last"
          ],
          "fields": ""
        },
        "orientation": "horizontal"
      }
    }
  ],
  "description": "Dependency auto-sync metrics, guard violations, and update trends."
}